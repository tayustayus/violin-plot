# set up necessary module
import pandas as pd
# set up the matploblib package
import matplotlib.pyplot as plt
x = [0,2,4,6,8]
y = [1,3,5,7,9]
z = [4,8,8,6,8]
plt.plot(x,y)
plt.plot(x,z)
plt.title("Pepesi Advert")
plt.xlabel("sales")
plt.ylabel("profit")
plt.legend(["My X", "My Y"])
plt.show()


#insert the necessary module
import seaborn as sns
#plotting a bar chart of the data above
plt.bar(x,y)
plt.bar(x,z)
plt.title("Pepesi Advert")
plt.xlabel("sales")
plt.ylabel("profit")
plt.legend(["My X", "My Y"])
plt.show()
